{
    "version": 2,
    "builds": [
      { "src": "landing/**/*.html", "use": "@vercel/static" },
      { "src": "landing/**/*.css", "use": "@vercel/static" },
      { "src": "landing/**/*.js", "use": "@vercel/node" },
      { "src": "landing/**/*.ts", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/favicon.png", "dest": "/public/favicon.png" },
      { "src": "/public/(.*)", "dest": "/public/$1" },
      { "handle": "filesystem" },
      { "src": "./*", "dest": "/landing/index.html" }
    ],
    "env": {
      "TAILWIND_MODE": "build"
    },
    "functions": { "api/**/*.ts": { "runtime": "js" } },
    "plugins": [
      { "src": "@vercel/plugin-typescript" },
      {
        "src": "@vercel/plugin-postcss",
        "config": { "postcss": { "plugins": { "postcss-import": {} } } }
      }
    ]
  }
  